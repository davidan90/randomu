{
  "name": "randomu",
  "version": "1.0.0",
  "description": "homeworks from Datio",
  "author": "David Alvarez",
  "license": "ISC",
  "main": "index.js",
  "keywords": [
    "react",
    "redux",
    "saga"
  ],
  "engines": {
    "node": "> 8",
    "npm": ">= 5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "scripts": {
    "start": "node ./config/webpack/dev/webpack-server.js",
    "build": "webpack --config ./config/webpack/prod/webpack.config.js",
    "lint": "eslint --ext .js,.jsx src",
    "lint:js:fix": "eslint --ext .js,.jsx src --fix",
    "lint:css": "stylelint '**/*.scss' --syntax scss",
    "test": "jest --config ./config/jest/jest.config.js",
    "test:watch": "jest --watchAll --coverage --config ./config/jest/jest.config.js",
    "test:overwrite:snap": "npm test -- -u",
    "coverage:open:mac": "open coverage/lcov-report/index.html",
    "coverage:open:linux": "xdg-open coverage/lcov-report/index.html",
    "precommit": "npm run lint && npm run lint:css"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "node-sass": "^4.9.2",
    "opn": "^5.3.0",
    "postcss": "^7.0.1",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.14.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  }
}
