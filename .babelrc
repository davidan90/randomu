{
  "presets": [
    "env",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "api": "./src/api",
          "components": "./src/components",
          "containers": "./src/containers",
          "routes": "./src/routes",
          "store": "./src/store"
        }
      }
    ]
  ]
}
